{
  "name": "docker-demo-backend",
  "version": "1.0.0",
  "description": "Simple NodeJS backend",
  "main": "index.js",
  "scripts": {
    "start": "nodemon -x --experimental-modules index.mjs",
    "start:watch": "nodemon --watch . --experimental-modules index.mjs",
    "seedDb": "node --experimental-modules seedDb.mjs",
    "seedDb:watch": "nodemon --watch seedDb.mjs --watch seedData.json --experimental-modules seedDb.mjs",
    "dev": "concurrently 'npm run start:watch' 'npm run seedDb:watch'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frogamic/docker-js-demo.git"
  },
  "keywords": [
    "docker",
    "backend",
    "server"
  ],
  "author": "Dominic Shelton",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/frogamic/docker-js-demo/issues"
  },
  "homepage": "https://github.com/frogamic/docker-js-demo#readme",
  "dependencies": {
    "express": "^4.17.1",
    "knex": "^0.18.0",
    "pg": "^7.11.0",
    "nodemon": "^1.19.1"
  },
  "devDependencies": {
    "concurrently": "^4.1.1"
  }
}
